import "std:feature_test";

when      feature_test.APPLE     { export "std:sys/apple/os"     }
else when feature_test.LINUX     { export "std:sys/linux/os"     }
else when feature_test.BSD       { export "std:sys/bsd/os"       }
else when feature_test.MICROSOFT { export "std:sys/microsoft/os" }
else {
	_ := compile_assert(false, "Unsupported platform!");
}