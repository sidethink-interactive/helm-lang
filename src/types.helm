export "gen/token_kinds"

File :: struct {
	path: string,
	contents: string,
	// Used for error logging,
	// not for lexing.
	owned_strings: [dynamic]^byte,
	lines: [dynamic]string,
	tokens: [dynamic]Token,
	//ast: Ast_Node
}

Token :: struct {
	kind: Token_Kind,
	file: ^File,
	line, col: u32,
	text: string,
}